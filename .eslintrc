{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "curly": [1, "multi-line"],
    "strict": [2, "never"],
    "block-scoped-var": 1,
    "no-throw-literal": 2,
    "radix": 1,
    "indent": [1, 2],
    "comma-style": [1, "last"],
    "key-spacing": 1,
    "eol-last": 1,
    "no-trailing-spaces": 1,
    "space-before-function-paren": [1, "never"],
    "space-after-keywords": [1, "always"],
    "space-before-blocks": [1, "always"],
    "space-in-brackets": [1, "never"],
    "space-in-parens": [1, "never"],
    "no-var": 1
  }
}